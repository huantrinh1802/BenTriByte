{
  "name": "skeleton-svelte-components",
  "version": "0.0.3",
  "license": "MIT",
  "author": "Ben Trinh <huantrinh1802@gmail.com>",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "story:dev": "histoire dev",
    "story:build": "histoire build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@histoire/plugin-svelte": "^0.17.17",
    "@skeletonlabs/skeleton": "2.6.1",
    "@skeletonlabs/tw-plugin": "^0.3.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.20.6",
    "@sveltejs/package": "^2.2.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "autoprefixer": "^10.4.15",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "histoire": "^0.17.17",
    "postcss": "^8.5.3",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "publint": "^0.1.16",
    "svelte": "^4.2.19",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^6.2.7"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "description": "Everything you need to build a Svelte library, powered by [`create-svelte`](https://github.com/sveltejs/kit/tree/master/packages/create-svelte).",
  "main": "svelte.config.js",
  "dependencies": {
    "eslint-scope": "^7.2.2",
    "eslint-utils": "^3.0.0",
    "eslint-visitor-keys": "^3.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huantrinh1802/kingcong_svelte_components.git"
  },
  "keywords": [
    "svelte",
    "svelte-components",
    "skeleton-components"
  ],
  "bugs": {
    "url": "https://github.com/huantrinh1802/kingcong_svelte_components/issues"
  },
  "homepage": "https://github.com/huantrinh1802/kingcong_svelte_components#readme"
}
