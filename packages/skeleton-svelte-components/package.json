{
  "name": "skeleton-svelte-components",
  "version": "0.0.2",
  "license": "MIT",
  "author": "Ben Trinh <huantrinh1802@gmail.com>",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "story:dev": "histoire dev",
    "story:build": "histoire build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@histoire/plugin-svelte": "^0.15.8",
    "@skeletonlabs/skeleton": "^1.0.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.11.0",
    "@sveltejs/package": "^2.0.0",
    "@tailwindcss/container-queries": "^0.1.0",
    "@tailwindcss/typography": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "histoire": "^0.15.8",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "publint": "^0.1.9",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "description": "Everything you need to build a Svelte library, powered by [`create-svelte`](https://github.com/sveltejs/kit/tree/master/packages/create-svelte).",
  "main": "svelte.config.js",
  "dependencies": {
    "eslint-visitor-keys": "^3.3.0",
    "eslint-utils": "^3.0.0",
    "eslint-scope": "^7.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huantrinh1802/kingcong_svelte_components.git"
  },
  "keywords": [
    "svelte",
    "svelte-components",
    "skeleton-components"
  ],
  "bugs": {
    "url": "https://github.com/huantrinh1802/kingcong_svelte_components/issues"
  },
  "homepage": "https://github.com/huantrinh1802/kingcong_svelte_components#readme"
}
